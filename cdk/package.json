{
  "name": "bedrock-chatbot-cdk",
  "version": "0.1.0",
  "bin": {
    "bedrock-chatbot": "bin/app.js"
  },
  "scripts": {
    "build": "tsc -p .",
    "watch": "tsc -w",
    "synth": "npm run build && cdk synth",
    "deploy": "npm run build && cdk deploy --all",
    "deploy:stack": "npm run build && cdk deploy",
    "diff": "npm run build && cdk diff"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "ts-node": "^10.9.1",
    "@types/node": "^24.7.0",
    "aws-cdk": "2.114.1"
  },
  "dependencies": {
    "aws-cdk-lib": "2.114.1",
    "constructs": "^10.4.2",
    "@aws-cdk/aws-apigatewayv2-alpha": "2.114.1-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.114.1-alpha.0",
    "esbuild": "^0.25.10",
    "@aws-sdk/client-sqs": "^3.901.0",
    "@aws-sdk/client-dynamodb": "^3.902.0",
    "@aws-sdk/client-bedrock-runtime": "^3.901.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.901.0",
    "@aws-sdk/client-apigatewaymanagementapi": "^3.901.0",
    "@aws-sdk/signature-v4": "^3.374.0",
    "@aws-sdk/protocol-http": "^3.374.0",
    "@aws-crypto/sha256-js": "^5.2.0",
    "@aws-sdk/credential-provider-node": "^3.374.0"
  }
}
